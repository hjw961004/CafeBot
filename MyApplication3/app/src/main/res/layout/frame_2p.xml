<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cfb">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="490dp">

        <ImageView
            android:id="@+id/img_logo"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/logo" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="42dp"
            android:layout_marginTop="32dp"
            android:layout_toRightOf="@+id/img_logo"
            android:text="Cafe List"
            android:textColor="@color/black"
            android:textSize="30dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/img_rect"
            android:layout_width="350dp"
            android:layout_height="340dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="33dp"
            android:layout_marginLeft="33dp"
            android:layout_marginTop="110dp"
            android:layout_marginEnd="28dp"
            android:layout_marginRight="28dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="@color/white" />

        <ImageView
            android:id="@+id/ts"
            android:layout_width="113dp"
            android:layout_height="119dp"
            android:layout_below="@id/img_logo"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="33dp"
            android:layout_marginLeft="33dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="265dp"
            android:layout_marginRight="265dp"
            android:background="@drawable/twosome" />

        <ImageView
            android:id="@+id/yoger"
            android:layout_width="115dp"
            android:layout_height="114dp"
            android:layout_below="@id/img_logo"
            android:layout_alignEnd="@+id/cbe"
            android:layout_alignRight="@+id/cbe"
            android:layout_marginStart="116dp"
            android:layout_marginLeft="116dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="118dp"
            android:layout_marginRight="118dp"
            android:background="@drawable/yogerpresso" />

        <ImageView
            android:id="@+id/cbe"
            android:layout_width="113dp"
            android:layout_height="114dp"
            android:layout_below="@id/img_logo"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="269dp"
            android:layout_marginLeft="269dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="-5dp"
            android:layout_marginRight="-5dp"
            android:background="@drawable/caffebene" />

        <ImageView
            android:id="@+id/gong"
            android:layout_width="91dp"
            android:layout_height="114dp"
            android:layout_below="@id/img_logo"
            android:layout_alignStart="@+id/img_rect"
            android:layout_alignLeft="@+id/img_rect"
            android:layout_alignEnd="@+id/img_rect"
            android:layout_alignRight="@+id/img_rect"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="143dp"
            android:layout_marginEnd="249dp"
            android:layout_marginRight="249dp"
            android:layout_marginBottom="117dp"
            android:background="@drawable/gongcha" />

        <ImageView
            android:id="@+id/mg"
            android:layout_width="109dp"
            android:layout_height="118dp"
            android:layout_below="@id/img_logo"
            android:layout_alignStart="@+id/img_rect"
            android:layout_alignLeft="@+id/img_rect"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="239dp"
            android:layout_marginLeft="239dp"
            android:layout_marginTop="137dp"
            android:layout_marginEnd="33dp"
            android:layout_marginRight="33dp"
            android:background="@drawable/mega" />

        <ImageView
            android:id="@+id/logo"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="155dp"
            android:layout_marginRight="155dp"
            android:layout_marginBottom="148dp"
            android:background="@drawable/logo" />

        <ImageView
            android:id="@+id/prev_btn"
            android:layout_width="35dp"
            android:layout_height="74dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="170dp"
            android:src="@drawable/left"
            android:textStyle="bold" />

    </RelativeLayout>
</FrameLayout>